{
  "name": "@lanseria/v-pdf",
  "version": "1.0.2",
  "description": "A Vue 3 Pdf Tools",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && npm run gen-version && cross-env NODE_ENV=development vite",
    "build:package": "npm run gen-version && npm run clean && tsc -b --force tsconfig.esm.json && node scripts/pre-build/pre-cjs-build.js && tsc -b --force tsconfig.cjs.json && node scripts/post-build",
    "build:site": "./scripts/pre-build-site/pre-build-site.sh && cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=4096 vite build && ./scripts/post-build-site/post-build-site.sh",
    "clean": "rimraf site lib es node_modules/v-pdf",
    "release:package": "npm run build:package && npm publish",
    "gen-version": "node scripts/gen-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lanseria/v-pdf.git"
  },
  "keywords": [
    "v-pdf",
    "component library",
    "pdf",
    "vue"
  ],
  "author": "Lanseria",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lanseria/v-pdf/issues"
  },
  "homepage": "https://github.com/Lanseria/v-pdf#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/generator": "^7.12.11",
    "@babel/parser": "^7.12.11",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@babel/traverse": "^7.12.12",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/lodash": "^4.14.170",
    "@types/lodash-es": "^4.17.4",
    "@types/pdfjs-dist": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "fs-extra": "^10.0.0",
    "highlight.js": "^11.0.1",
    "marked": "^2.1.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue": "^3.0.10",
    "vue-router": "^4.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^5.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "pdfjs-dist": "^2.8.335"
  }
}
